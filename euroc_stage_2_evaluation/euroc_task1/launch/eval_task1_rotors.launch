<launch>
  <arg name="mav_name" default="firefly" />

  <node name="task1_eval_node" pkg="euroc_task1" type="task1_eval_node" output="screen">
    <remap from="vicon_transform" to="/$(arg mav_name)/ground_truth/transform" />
    <rosparam file="$(find euroc_task1)/parameters/task1_params.yaml"/>
  </node>

  <include file="$(find mav_saver)/launch/mav_saver_rotors.launch" />
</launch>
