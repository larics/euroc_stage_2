<launch>
  <arg name="namespace" default="firefly"/>
  <group ns="$(arg namespace)">
    <node name="mav_linear_mpc" pkg="mav_linear_mpc" type="mav_linear_mpc_node"  respawn="true" clear_params="true" output="screen">
      <remap from="odometry" to="msf_core/odometry" />
      <!--remap from="command/roll_pitch_yawrate_thrust" to="command/roll_pitch_yawrate_thrust"/-->
      <rosparam file="$(find mav_linear_mpc)/parameters/linear_mpc.yaml"/>
    </node>
  </group>
</launch>
